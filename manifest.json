{
  "manifest_version": 3,
  "name": "boost-note",
  "version": "0",
  "description": "Boost note.com",
  "icons": {
    "128": "128.png"
  },
  "content_scripts": [{
    "css": ["main.css"],
    "js": ["main.js"],
    "matches": ["https://note.com/*"],
    "world": "MAIN",
    "run_at": "document_start"
  }]
}